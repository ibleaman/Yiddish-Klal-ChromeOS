{
  "name": "Yiddish Klal",
  "version": "0.5",
  "manifest_version": 3,
  "author": "Isaac L. Bleaman",
  "description": "A phonetic (QWERTY) keyboard layout for Standard Yiddish.",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "input"
  ],
  "input_components": [
    {
      "name": "Yiddish Klal",
      "type": "ime",
      "id": "yiddishklal",
      "description": "Yiddish Klal",
      "language": "en-US",
      "layouts": ["us::eng"]
    }
  ]
}